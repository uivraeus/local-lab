{
  "cSpell.words": [
  ],
  "cSpell.enableFiletypes": [
    "markdown",
    "python",
    "shellscript",
    "golang"
  ],
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "[python]" : {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "remote.restoreForwardedPorts": false,
}
